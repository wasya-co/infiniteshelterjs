  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.15.6",
    "@capacitor/cli": "^3.2.4",
    "@capacitor/core": "^3.2.4",
    "@ionic/cli": "^6.17.1",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "babel-jest": "^27.2.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "cordova-res": "^0.15.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "ionic": "^5.4.16",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "link-module-alias": "^1.2.0",
    "native-run": "^1.0.0",
    "react-scripts": "4.0.3"
  },
  "_moduleAliases": {
    "$components": "src/components",
    "$lib": "src/lib",
    "$resources": "src/resources",
    "$shared": "src/shared",
    "$src": "src"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "./config/browserMock.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}",
      "**/*_spec.jsx"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@capacitor-community|three)/)"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "\\.(css|less|sass|scss)$": "<rootDir>/config/styleMock.js",
      "\\.(eot|gif|jpg|jpeg|png|svg|ttf)$": "<rootDir>/config/fileMock.js",
      "^\\$components(.*)\\.jsx$": "<rootDir>/src/components$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "scripts": {
    "build": "echo '+++ +++' ; echo +++ +++ 'build_guydme (@deprecated, see ./scripts/build), build_ish, build_staging_ish (untested)?' ; echo '+++ +++'",
    "build_guydme":      "NODE_PATH=config/environments/production_guydme:shared_modules node scripts/build.js",
    "build_ish":         "NODE_PATH=config/environments/production_ish:shared_modules    node scripts/build.js",
    "jest": "NODE_PATH=config/environments/test:shared_modules jest -t current",
    "jest2": "echo '+++ +++ usage:' ; echo '  yarn run jest2 -- <file>' ; echo '' ; NODE_PATH=config/environments/test:shared_modules jest -t current2",
    "next_build": "NODE_PATH=config/environments/production_ish:shared_modules next build",
    "next_dev": "NODE_PATH=config/environments/development_web:shared_modules next dev -- --port 3004",
    "next_start": "next start -- --port 8091",
    "nft_cancel": "npx hardhat run scripts/nft-cancel.js --network ropsten",
    "nft_compile": "npx hardhat compile",
    "nft_deploy": "npx hardhat run scripts/nft-deploy.js --network ropsten",
    "nft_mint": "npx hardhat run scripts/nft-mint.js   --network ropsten",
    "nft_test": "npx hardhat test",
    "postinstall": "link-module-alias",
    "resources": "cordova-res ios && cordova-res android && node scripts/resources.js",
    "start": "echo '+++ +++'; echo '+++ +++ start_local , or start_devremote ?'; echo '+++ +++'",
    "start_devremote": "        HTTPS=true  HOST=tgm.mac             PORT=3002  NODE_PATH=config/environments/devremote:shared_modules               node scripts/start.js",
    "start_local": "            HTTPS=true                           PORT=3002  NODE_PATH=config/environments/development_web:shared_modules         node scripts/start.js",
    "start_local2": "           HTTPS=true  HOST=infiniteshelter.mac PORT=3002  NODE_PATH=config/environments/development_web:shared_modules         SSL_CRT_FILE=config/ssl/infiniteshelter.mac.pem SSL_KEY_FILE=config/ssl/infiniteshelter.mac-key.pem node scripts/start.js",
    "start_local_guydme": "     HTTPS=true  HOST=tgm.mac             PORT=3002  NODE_PATH=config/environments/development_web_guydme:shared_modules  node scripts/start.js",
    "start_local_ish": "        HTTPS=true  HOST=tgm.mac             PORT=3002  NODE_PATH=config/environments/development_web_ish:shared_modules     node scripts/start.js",
    "start_mobile_trash": "     HTTPS=true  HOST=tgm.mac             PORT=3002  NODE_PATH=config/environments/development_mobile:shared_modules      node scripts/start.js",
    "start_production_guydme": "HTTPS=true  HOST=tgm.mac             PORT=3002  NODE_PATH=config/environments/production_guydme:shared_modules       node scripts/start.js",
    "sync": "NODE_PATH=config/environments/production:shared_modules ionic capacitor sync",
    "test": "NODE_OPTIONS=--max_old_space_size=4096 NODE_PATH=config/environments/test:shared_modules       jest --maxWorkers=20% "
  }
}
