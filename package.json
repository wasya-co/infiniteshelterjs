{
  "name": "guyd",
  "description": "An Ionic project",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@capacitor/android": "^2.2.0",
    "@capacitor/core": "2.2.0",
    "@capacitor/ios": "^2.2.0",
    "@ionic/react": "^5.0.7",
    "@ionic/react-router": "^5.0.7",
    "@material-ui/core": "^4.11.0",
    "axios": "^0.19.2",
    "ionicons": "^5.0.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-facebook-login": "^4.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "2.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "cordova-res": "^0.15.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "link-module-alias": "^1.2.0"
  },
  "_moduleAliases": {
    "$components": "src/components",
    "$shared": "src/shared",
    "$src": "src"
  },
  "jest": {
    "moduleMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/spec/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/spec/fileMock.js"
    }
  },
  "scripts": {
    "start": "HTTPS=true NODE_PATH=config/environments/development PORT=3002 react-scripts start",
    "build": "NODE_PATH=config/environments/production react-scripts build",
    "test": "NODE_PATH=config/environments/test jest spec",
    "eject": "react-scripts eject",
    "postinstall": "link-module-alias",
    "preinstall_trash": "command -v link-module-alias && link-module-alias clean || true",
    "resources": "cordova-res ios && cordova-res android && node scripts/resources.js"
  }
}
