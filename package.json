  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.15.6",
    "@capacitor/cli": "^3.2.4",
    "@capacitor/core": "^3.2.4",
    "@ionic/cli": "^6.17.1",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "babel-jest": "^27.2.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "cordova-res": "^0.15.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "ionic": "^5.4.16",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "link-module-alias": "^1.2.0",
    "native-run": "^1.0.0",
    "react-scripts": "4.0.3"
  },
  "_moduleAliases": {
    "$components": "src/components",
    "$lib": "src/lib",
    "$shared": "src/shared",
    "$src": "src"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "./config/browserMock.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}",
      "**/*_spec.jsx"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transformTrash": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@capacitor-community|three)/)"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^\\$components(.*)$": "<rootDir>/src/components$1",
      "\\.(css|less|sass|scss)$": "<rootDir>/config/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/config/fileMock.js"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "scripts": {
    "build": "echo +++ +++\n\r'build_guydme , or build_ish ?\n\r @TODO: this isn't wired. +++ +++'",
    "build_guydme": "NODE_PATH=config/environments/production_guydme node scripts/build.js",
    "build_ish":    "NODE_PATH=config/environments/production_ish    node scripts/build.js",
    "jest":         "NODE_PATH=config/environments/test              jest -t current",
    "nft_cancel": "npx hardhat run scripts/nft-cancel.js --network ropsten",
    "nft_compile": "npx hardhat compile",
    "nft_deploy": "npx hardhat run scripts/nft-deploy.js --network ropsten",
    "nft_mint": "npx hardhat run scripts/nft-mint.js   --network ropsten",
    "nft_test": "npx hardhat test",
    "postinstall": "link-module-alias",
    "resources": "cordova-res ios && cordova-res android && node scripts/resources.js",
    "start": "echo +++ +++\n\r'start_local , or start_devremote ?\n\r+++ +++'",
    "start_devremote": "HTTPS=true HOST=tgm.mac PORT=3002 NODE_PATH=config/environments/devremote              node scripts/start.js",
    "start_local": "HTTPS=true HOST=tgm.mac PORT=3002 NODE_PATH=config/environments/development_web        node scripts/start.js",
    "start_local_guydme": "HTTPS=true HOST=tgm.mac PORT=3002 NODE_PATH=config/environments/development_web_guydme node scripts/start.js",
    "start_local_ish": "HTTPS=true HOST=tgm.mac PORT=3002 NODE_PATH=config/environments/development_web_ish    node scripts/start.js",
    "start_mobile_trash": "HTTPS=true HOST=tgm.mac PORT=3002 NODE_PATH=config/environments/development_mobile     node scripts/start.js",
    "start_production_guydme": "HTTPS=true HOST=tgm.mac PORT=3002 NODE_PATH=config/environments/production_guydme      node scripts/start.js",
    "sync": "NODE_PATH=config/environments/production ionic capacitor sync",
    "test": "NODE_PATH=config/environments/test       jest"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
