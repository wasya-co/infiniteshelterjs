{
  "name": "guyd",
  "description": "An Ionic project",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@capacitor-community/facebook-login": "^1.0.1",
    "@capacitor/android": "^2.5.0",
    "@ionic/react": "^5.0.7",
    "@ionic/react-router": "^5.0.7",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.8.0",
    "axios": "^0.19.2",
    "ionicons": "^5.0.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-facebook-login": "^4.1.1",
    "react-modal": "^3.11.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@capacitor/cli": "^2.5.0",
    "@capacitor/core": "^2.5.0",
    "@ionic/cli": "^6.17.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "cordova-res": "^0.15.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "ionic": "^5.4.16",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "link-module-alias": "^1.2.0",
    "native-run": "^1.0.0"
  },
  "_moduleAliases": {
    "$components": "src/components",
    "$shared": "src/shared",
    "$src": "src"
  },
  "jest": {
    "moduleNameMapper": {
      "^\\$components(.*)$": "<rootDir>/src/components$1",
      "\\.(css|less|sass|scss)$": "<rootDir>/config/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/config/fileMock.js"
    },
    "roots": [
      "src"
    ],
    "setupFiles": [
      "./config/browserMock.js"
    ],
    "testMatch": [
      "**/*_spec.jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!@capacitor-community)"
    ]
  },
  "scripts": {
    "jest": "NODE_PATH=config/environments/test jest -t current",
    "start": "HTTPS=true NODE_PATH=config/environments/development PORT=3002 react-scripts start",
    "start_do": "HTTPS=true NODE_PATH=config/environments/production PORT=3002 react-scripts start",
    "build": "NODE_PATH=config/environments/production react-scripts build",
    "test": "NODE_PATH=config/environments/test jest",
    "eject": "react-scripts eject",
    "postinstall": "link-module-alias",
    "preinstall_trash": "command -v link-module-alias && link-module-alias clean || true",
    "resources": "cordova-res ios && cordova-res android && node scripts/resources.js"
  }
}
